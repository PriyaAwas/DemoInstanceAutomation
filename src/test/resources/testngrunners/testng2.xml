<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Test Execution">

    <listeners>
        <listener class-name="sew.ai.helpers.listeners.TestListeners"/>
        <listener class-name="sew.ai.helpers.listeners.MethodInterceptor"/>
        <listener class-name="sew.ai.helpers.listeners.AnnotationTransformer"/>
        <listener class-name="sew.ai.helpers.testrail.TestRailListener"/>
    </listeners>

    <test name="E2E Test Suite On Web - Chrome">
        <parameter name="platformType" value="web"/>
